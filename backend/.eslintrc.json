{
    "env": {
        "node": true,
        "browser": false,
        "es2021": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "globals": {
        "process": "readonly"
    },
    "rules": {
        "no-unused-vars": "warn",
        "no-console": "off"
    }
}